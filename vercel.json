{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "headers": [
    {
      "source": "/(.*)", 
      "destination": "dist/index.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*" 
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS, POST, PUT, DELETE" 
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
  